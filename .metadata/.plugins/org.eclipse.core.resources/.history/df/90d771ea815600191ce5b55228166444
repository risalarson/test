
public class Robot {

	public String name=null;
	public String color=null;
	public void size(){
		System.out.println(this.size+"mm");
	}
	public void talk(){
		System.out.println(this.name+"が話す");
	}
	public void walk(){
		System.out.println(this.name+"が歩く");
	}
	public void run(){
		System.out.println(this.name+"が走る");
	}

}
